<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="#ffffff"
android:orientation="vertical"
tools:context="com.example.admin.quizapp.MainActivity">
<ScrollView
    android:layout_width="match_parent"
    android:layout_weight="20"
    android:layout_height="0dp"
    tools:ignore="UselessParent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:id="@+id/layout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginTop="0dp"
                android:layout_gravity="center"
                android:layout_weight="3"
                android:src="@drawable/q1"
                tools:ignore="ContentDescription" />
            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_weight="6"
                android:textSize="16sp"
                android:background="@drawable/rounded_corner1"
                android:gravity="center"
                android:padding="16dp"
                android:text="In what year google was founded?"
                android:textColor="#ffffff"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/layout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_below="@id/layout1"
            android:orientation="horizontal">
            <RadioGroup xmlns:android="http://schemas.android.com/apk/res/android"
                android:background="@drawable/rounded_corner"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:layout_weight="6"
                android:padding="16dp">
                <RadioButton android:id="@+id/a2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/a2"
                    android:onClick="onRadioButtonClicked"/>
                <RadioButton android:id="@+id/a3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/a3"
                    android:onClick="onRadioButtonClicked"/>
                <RadioButton android:id="@+id/a4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/a4"
                    android:onClick="onRadioButtonClicked"/>
            </RadioGroup>
            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginTop="0dp"
                android:layout_weight="3"
                android:src="@drawable/udacity"
                tools:ignore="ContentDescription" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_below="@id/layout2"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            android:layout_marginTop="16dp">
        </LinearLayout>
        <LinearLayout
            android:id="@+id/layout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/layout2"
            android:layout_marginTop="30dp"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:layout_height="40dp"
                android:layout_marginTop="0dp"
                android:layout_weight="3"
                android:src="@drawable/q2"
                tools:ignore="ContentDescription" />
            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:layout_marginTop="10dp"
                android:textSize="16sp"
                android:layout_weight="6"
                android:background="@drawable/rounded_corner1"
                android:padding="16dp"
                android:text="Who are the founders of google?"
                android:textColor="#ffffff"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/layout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/layout3"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginStart="10dp"
                android:layout_weight="6"
                android:background="@drawable/rounded_corner"
                android:padding="16dp"
                android:orientation="vertical">
                <LinearLayout
                    android:gravity="center"
                    android:layout_marginBottom="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
            <CheckBox android:id="@+id/b1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/b1"
                android:textColor="#000"
                android:onClick="onCheckboxClicked"/>
            <CheckBox android:id="@+id/b2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000"
                android:text="@string/b2"
                android:onClick="onCheckboxClicked"/>
                </LinearLayout>
                <LinearLayout
                    android:gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
            <CheckBox android:id="@+id/b3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/b3"
                android:textColor="#000"
                android:onClick="onCheckboxClicked"/>
            <CheckBox android:id="@+id/b4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000"
                android:text="@string/b4"
                android:onClick="onCheckboxClicked"/>
                </LinearLayout>
            </LinearLayout>
            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginTop="0dp"
                android:layout_weight="3"
                android:src="@drawable/udacity"
                tools:ignore="ContentDescription" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_below="@id/layout4"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            android:layout_marginTop="16dp">
        </LinearLayout>
        <LinearLayout
            android:id="@+id/layout5"
            android:layout_below="@id/layout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:layout_marginTop="30dp"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginTop="0dp"
                android:layout_weight="3"
                android:src="@drawable/q3"
                tools:ignore="ContentDescription" />
            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:textSize="16sp"
                android:layout_weight="6"
                android:background="@drawable/rounded_corner1"
                android:padding="16dp"
                android:text="In what year udacity was founded?"
                android:textColor="#ffffff"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/layout6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_below="@id/layout5"
            android:orientation="horizontal">
            <RadioGroup xmlns:android="http://schemas.android.com/apk/res/android"
                android:background="@drawable/rounded_corner"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:layout_weight="6"
                android:padding="16dp">
                <RadioButton android:id="@+id/c1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/c1"
                    android:onClick="onRadioButtonClicked"/>
                <RadioButton android:id="@+id/c2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/c2"
                    android:onClick="onRadioButtonClicked"/>
                <RadioButton android:id="@+id/c3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/c3"
                    android:onClick="onRadioButtonClicked"/>
            </RadioGroup>
            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginTop="0dp"
                android:layout_weight="3"
                android:src="@drawable/udacity"
                tools:ignore="ContentDescription" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_below="@id/layout6"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            android:layout_marginTop="16dp">
        </LinearLayout>
        <LinearLayout
            android:id="@+id/layout7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/layout6"
            android:layout_marginTop="30dp"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:layout_height="40dp"
                android:layout_marginTop="0dp"
                android:layout_weight="3"
                android:src="@drawable/q4"
                tools:ignore="ContentDescription" />
            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:layout_marginTop="10dp"
                android:layout_weight="6"
                android:background="@drawable/rounded_corner1"
                android:padding="16dp"
                android:textSize="16sp"
                android:text="Rick Astley is never gonna:"
                android:textColor="#ffffff"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/layout8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/layout7"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginStart="10dp"
                android:layout_weight="6"
                android:background="@drawable/rounded_corner"
                android:padding="16dp"
                android:orientation="vertical">
                    <CheckBox android:id="@+id/d1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/d1"
                        android:textColor="#000"
                        android:onClick="onCheckboxClicked"
                        />
                    <CheckBox android:id="@+id/d2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#000"
                        android:text="@string/d2"
                        android:onClick="onCheckboxClicked"
                        />
                    <CheckBox android:id="@+id/d3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/d3"
                        android:textColor="#000"
                        android:onClick="onCheckboxClicked"
                        />
                    <CheckBox android:id="@+id/d4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#000"
                        android:text="@string/d4"
                        android:onClick="onCheckboxClicked"
                        />
            </LinearLayout>
            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:layout_height="40dp"
                android:layout_marginTop="0dp"
                android:layout_weight="3"
                android:src="@drawable/udacity"
                tools:ignore="ContentDescription" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_below="@id/layout8"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            android:layout_marginTop="16dp">
        </LinearLayout>
        <LinearLayout
            android:id="@+id/layout9"
            android:layout_below="@id/layout8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:layout_marginTop="30dp"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/imageView17"
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:layout_height="40dp"
                android:layout_marginTop="0dp"
                android:layout_weight="3"
                android:src="@drawable/q5"
                tools:ignore="ContentDescription" />
            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_weight="6"
                android:background="@drawable/rounded_corner1"
                android:padding="16dp"
                android:textSize="16sp"
                android:text="Who shot first?"
                android:textColor="#ffffff"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/layout10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_below="@id/layout9"
            android:orientation="horizontal">
            <RadioGroup xmlns:android="http://schemas.android.com/apk/res/android"
                android:background="@drawable/rounded_corner"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:layout_weight="6"
                android:padding="16dp">
                <RadioButton android:id="@+id/e1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/e1"
                    android:onClick="onRadioButtonClicked"/>
                <RadioButton android:id="@+id/e2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/e2"
                    android:onClick="onRadioButtonClicked"/>
            </RadioGroup>
            <ImageView
                android:id="@+id/imageView20"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginTop="0dp"
                android:layout_weight="3"
                android:src="@drawable/udacity"
                tools:ignore="ContentDescription" />
        </LinearLayout>
        <EditText
            android:layout_marginTop="30dp"
            android:layout_below="@id/layout10"
            android:id="@+id/plain_text_input"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:hint="Please Enter Your Name"
            android:textColor="#000"
            android:inputType="text"
            tools:ignore="HardcodedText" />
        <Button
            android:id="@+id/button_id"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:layout_marginTop="20dp"
            android:textColor="#ffffff"
            android:textStyle="bold"
            android:textSize="16sp"
            android:layout_below="@id/plain_text_input"
            android:text="Submit"
            tools:ignore="HardcodedText" />
    </RelativeLayout>
</ScrollView>
</LinearLayout>